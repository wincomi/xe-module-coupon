<query id="getTicket" action="select">
    <tables>
        <table name="ticket" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="ticket_srl" var="ticket_srl" pipe="and" />
        <condition operation="equal" column="ticket_type" var="ticket_type" pipe="and" />
        <condition operation="equal" column="member_srl" var="member_srl" pipe="and" />
    </conditions>
</query>
