<query id="deleteCouponUsedLogs" action="delete">
    <tables>
        <table name="coupon" />
    </tables>
    <conditions>
        <condition operation="in" column="log_srl" var="log_srl" pipe="or" />
        <condition operation="in" column="coupon_srl" var="coupon_srl" pipe="or" />
    </conditions>
</query>